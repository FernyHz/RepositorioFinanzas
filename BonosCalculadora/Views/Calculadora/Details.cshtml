@model BonosCalculadora.Models.Calculadora

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<form>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Frecuencia del Cupon: </label>
        <div class="col-sm-10">
            <label type="text" readonly class="form-control-plaintext"> @ViewBag.Frec</label>
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Dias de Capitalizacion: </label>
        <div class="col-sm-10">
            <label type="text" readonly class="form-control-plaintext"> @ViewBag.Capi</label>
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Periodos Por Año: </label>
        <div class="col-sm-10">
            <label type="text" readonly class="form-control-plaintext"> @ViewBag.Peri</label>
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Periodos Totales: </label>
        <div class="col-sm-10">
            <label type="text" readonly class="form-control-plaintext"> @ViewBag.PeriT</label>
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Tasa Efectiva Anual: </label>
        <div class="col-sm-10">
            <label type="text" readonly class="form-control-plaintext"> @ViewBag.Tasa</label>
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Tasa Efectiva Del Periodo: </label>
        <div class="col-sm-10">
            <label type="text" readonly class="form-control-plaintext"> @ViewBag.TasaP</label>
        </div>
    </div>

</form>
